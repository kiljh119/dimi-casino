{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "src/app.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "src/app.js"
      },
      {
        "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico)$)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ]
  }